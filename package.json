{
  "name": "gochen-r",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "npm run clean",
    "build": "cross-env NODE_ENV=production webpack -p",
    "clean": "rimraf dist/",
    "dev:server": "cross-env NODE_ENV=development PORT=3000 nodemon bin/app.js",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server -d --inline --hot --progress --no-info",
    "devv": "concurrently -r \"npm run dev:client\" \"npm run dev:server\"",
    "dev": "concurrently -r \"npm run dev:server\"",
    "lint": "npm run lint-ts && npm run lint-js && npm run lint-css",
    "lint-css": "stylelint \"./src/**/*.css\"",
    "lint-ts": "tslint \"src/**/*.tsx\" \"src/**/*.ts\" \"test/**/*.tsx\" \"test/**/*.ts\"",
    "lint-js": "eslint \"**/*.js\" --ignore-path .gitignore",
    "postinstall": "npm run typings && npm run build",
    "start": "cross-env NODE_ENV=production node server/node-server.js",
    "pretest": "npm run lint",
    "test": "karma start --singleRun true",
    "test:watch": "karma start --singleRun false",
    "posttest": "istanbul check-coverage --statement 75 --branch 50 --function 50",
    "typings": "rimraf typings/ && typings install"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.15.2",
    "express": "^4.14.0",
    "helmet": "^3.1.0",
    "immutable": "^3.8.1",
    "ramda": "^0.22.1",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-redux": "^5.0.0-beta.3",
    "react-router": "^4.0.0-alpha.5",
    "react-router-redux": "^4.0.0-rc.2",
    "redux": "^3.6.0",
    "redux-observable": "^0.12.1",
    "redux-thunk": "^2.1.0",
    "remarkable": "^1.7.1",
    "rxjs": "^5.0.0-rc.2",
    "typescript": "^2.0.7",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "@types/react": "^0.14.44",
    "@types/react-dom": "^0.14.18",
    "autoprefixer": "^6.5.2",
    "awesome-typescript-loader": "^3.0.0-beta.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "babel-register": "^6.18.0",
    "concurrently": "^3.1.0",
    "copy-webpack-plugin": "^4.0.0",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "cssnano": "^3.8.0",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "html-webpack-plugin": "^2.24.1",
    "json-loader": "^0.5.4",
    "postcss-cssnext": "^2.8.0",
    "postcss-import": "^8.1.3",
    "postcss-loader": "^1.1.0",
    "postcss-modules-local-by-default": "^1.1.1",
    "raw-loader": "^0.5.1",
    "redux-devtools": "^3.3.1",
    "rimraf": "^2.5.4",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "stylelint-webpack-plugin": "^0.4.0",
    "ts-loader": "^1.0.0",
    "tslint-loader": "^2.1.5",
    "url-loader": "^0.5.7",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.1",
    "webpack-split-by-path": "0.1.0-beta.1"
  }
}
